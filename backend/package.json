{
  "name": "zero-to-running-backend",
  "version": "1.0.0",
  "description": "Node.js API server built with Express.js framework for Zero-to-Running",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
